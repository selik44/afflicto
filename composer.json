{
	"name": "gentlefox/friluft",
	"description": "Friluft Multi-Store.",
	"license": "proprietary",
	"type": "project",
	"minimum-stability": "stable",
	"require": {
		"laravel/framework": "5.1.*",
		"Curl/Curl": "~1.2",
		"gentlefox/laratables": "~1.2",
		"barryvdh/laravel-ide-helper": "~2.0",
		"davejamesmiller/laravel-breadcrumbs": "~3.0",
		"doctrine/dbal": "~2.5",
		"nicolaslopezj/searchable": "~1.5",
		"laracasts/utilities": "~2.0",
		"anahkiasen/former": "~4.0@dev",
		"klarna/checkout": "~4.0",
		"klarna/php-xmlrpc": "^4.0",
		"knplabs/knp-snappy": "^0.4.0",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "h4cc/wkhtmltoimage-i386": "0.12.x",
        "fedeisas/laravel-mail-css-inliner": "@dev",
        "skovmand/mailchimp-laravel": "1.*",
		"intervention/image": "^2.3",
		"jenssegers/agent": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"repositories": [
      {
        "type": "vcs",
        "url": "https://afflicto@bitbucket.org/afflicto/neatdatatables.git"
      },
      {
      	"type": "vcs",
      	"url": "https://afflicto@bitbucket.org/gentlefox/laratables.git"
      },
      {
      	"type": "vcs",
      	"url": "https://afflicto@bitbucket.org/afflicto/html.git"
      }
    ],
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Friluft\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
